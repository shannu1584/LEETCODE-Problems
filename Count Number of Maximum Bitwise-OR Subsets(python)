class Solution(object):
    def countMaxOrSubsets(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        max_or = 0
        count = 0
        for i in range(1 << len(nums)):
            current_or = 0
            subset_size = 0
            for j in range(len(nums)):
                if (i >> j) & 1:
                    current_or |= nums[j]
                    subset_size += 1
            if subset_size > 0:
                if current_or > max_or:
                    max_or = current_or
                    count = 1
                elif current_or == max_or:
                    count += 1
        return count
